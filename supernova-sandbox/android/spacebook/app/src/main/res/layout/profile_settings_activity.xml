<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/profile_settings_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/profile_settings_activity_profile_settings_constraint_layout_background_color">

	<android.support.constraint.ConstraintLayout
		android:id="@+id/information_constraint_layout"
		android:layout_width="0dp"
		android:layout_height="@dimen/profile_settings_activity_information_constraint_layout_height"
		android:layout_marginTop="@dimen/profile_settings_activity_information_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/avatar_astronaut_image_view"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="199dp">
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/ui_settings_cell_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/profile_settings_activity_ui_settings_cell_constraint_layout_height"
			android:layout_marginTop="@dimen/profile_settings_activity_ui_settings_cell_constraint_layout_margin_top"
			android:background="@color/profile_settings_activity_ui_settings_cell_constraint_layout_background_color"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/details_text_view"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="25dp">
		
			<ImageView
				android:id="@+id/path2_image_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_path2_image_view_margin_end"
				android:scaleType="center"
				android:src="@drawable/path_2"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="348dp"
				tools:layout_editor_absoluteY="23dp"/>
		
			<TextView
				android:id="@+id/text_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_text_text_view_margin_end"
				android:fontFamily="@font/font_lato_bold"
				android:gravity="end"
				android:text="@string/profile_settings_activity_text_text_view_text"
				android:textColor="@color/profile_settings_activity_text_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_text_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toLeftOf="@+id/path2_image_view"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="260dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<TextView
				android:id="@+id/label_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_text_view_margin_start"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_text_view_text"
				android:textColor="@color/profile_settings_activity_label_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="20dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/group3_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/profile_settings_activity_group3_constraint_layout_height"
			android:layout_marginTop="@dimen/profile_settings_activity_group3_constraint_layout_margin_top"
			android:background="@color/profile_settings_activity_group3_constraint_layout_background_color"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/information_text_view"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="200dp">
		
			<TextView
				android:id="@+id/text_two_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_text_two_text_view_margin_end"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="end"
				android:text="@string/profile_settings_activity_text_two_text_view_text"
				android:textColor="@color/profile_settings_activity_text_two_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_text_two_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="330dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<TextView
				android:id="@+id/label_two_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_two_text_view_margin_start"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_two_text_view_text"
				android:textColor="@color/profile_settings_activity_label_two_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_two_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="20dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/group_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/profile_settings_activity_group_constraint_layout_height"
			android:background="@color/profile_settings_activity_group_constraint_layout_background_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="320dp">
		
			<TextView
				android:id="@+id/text_three_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_text_three_text_view_margin_end"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="end"
				android:text="@string/profile_settings_activity_text_three_text_view_text"
				android:textColor="@color/profile_settings_activity_text_three_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_text_three_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="303dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<TextView
				android:id="@+id/label_three_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_three_text_view_margin_start"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_three_text_view_text"
				android:textColor="@color/profile_settings_activity_label_three_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_three_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toRightOf="@+id/label_four_text_view"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="99dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<TextView
				android:id="@+id/label_four_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_four_text_view_margin_start"
				android:alpha="0.4"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_four_text_view_text"
				android:textColor="@color/profile_settings_activity_label_four_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_four_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="20dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/ui_settings_cell_two_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/profile_settings_activity_ui_settings_cell_two_constraint_layout_height"
			android:background="@color/profile_settings_activity_ui_settings_cell_two_constraint_layout_background_color"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/ui_settings_cell_constraint_layout"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="85dp">
		
			<ImageView
				android:id="@+id/path2_two_image_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_path2_two_image_view_margin_end"
				android:scaleType="center"
				android:src="@drawable/path_2"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="348dp"
				tools:layout_editor_absoluteY="23dp"/>
		
			<TextView
				android:id="@+id/text_four_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_text_four_text_view_margin_end"
				android:fontFamily="@font/font_lato_bold"
				android:gravity="end"
				android:text="@string/profile_settings_activity_text_four_text_view_text"
				android:textColor="@color/profile_settings_activity_text_four_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_text_four_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toLeftOf="@+id/path2_two_image_view"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="195dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<TextView
				android:id="@+id/label_five_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_five_text_view_margin_start"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_five_text_view_text"
				android:textColor="@color/profile_settings_activity_label_five_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_five_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="20dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<TextView
			android:id="@+id/details_text_view"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/profile_settings_activity_details_text_view_margin_start"
			android:layout_marginRight="@dimen/profile_settings_activity_details_text_view_margin_end"
			android:alpha="0.4"
			android:fontFamily="@font/font_lato_regular"
			android:gravity="start"
			android:letterSpacing="-0"
			android:text="@string/profile_settings_activity_details_text_view_text"
			android:textColor="@color/profile_settings_activity_details_text_view_text_color"
			android:textSize="@dimen/profile_settings_activity_details_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="20dp"
			tools:layout_editor_absoluteY="0dp"/>
	
		<TextView
			android:id="@+id/information_text_view"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/profile_settings_activity_information_text_view_margin_start"
			android:layout_marginRight="@dimen/profile_settings_activity_information_text_view_margin_end"
			android:layout_marginTop="@dimen/profile_settings_activity_information_text_view_margin_top"
			android:alpha="0.4"
			android:fontFamily="@font/font_lato_regular"
			android:gravity="start"
			android:letterSpacing="-0"
			android:text="@string/profile_settings_activity_information_text_view_text"
			android:textColor="@color/profile_settings_activity_information_text_view_text_color"
			android:textSize="@dimen/profile_settings_activity_information_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/ui_settings_cell_two_constraint_layout"
			tools:layout_editor_absoluteX="20dp"
			tools:layout_editor_absoluteY="175dp"/>
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/group2_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/profile_settings_activity_group2_constraint_layout_height"
			android:background="@color/profile_settings_activity_group2_constraint_layout_background_color"
			app:layout_constraintBottom_toTopOf="@+id/group_constraint_layout"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="260dp">
		
			<TextView
				android:id="@+id/label_six_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/profile_settings_activity_label_six_text_view_margin_start"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:text="@string/profile_settings_activity_label_six_text_view_text"
				android:textColor="@color/profile_settings_activity_label_six_text_view_text_color"
				android:textSize="@dimen/profile_settings_activity_label_six_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="20dp"/>
		
			<Switch
				android:id="@+id/slide_switch"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/profile_settings_activity_slide_switch_margin_end"
				android:background="@drawable/profile_settings_activity_slide_switch_background"
				android:checked="true"
				android:theme="@style/ProfileSettingsActivitySlideSwitchTheme"
				android:thumbTint="@color/profile_settings_activity_slide_switch_thumb_tint"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="304dp"
				tools:layout_editor_absoluteY="14dp"/>
		</android.support.constraint.ConstraintLayout>
	</android.support.constraint.ConstraintLayout>

	<ImageView
		android:id="@+id/avatar_astronaut_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="@dimen/profile_settings_activity_avatar_astronaut_image_view_margin_start"
		android:layout_marginRight="@dimen/profile_settings_activity_avatar_astronaut_image_view_margin_end"
		android:layout_marginTop="@dimen/profile_settings_activity_avatar_astronaut_image_view_margin_top"
		android:scaleType="center"
		android:src="@drawable/avatarastronaut_10"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="138dp"
		tools:layout_editor_absoluteY="89dp"/>

	<android.support.v7.widget.Toolbar
		android:id="@+id/toolbar"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>