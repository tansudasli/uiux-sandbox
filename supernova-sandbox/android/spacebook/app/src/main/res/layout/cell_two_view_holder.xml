<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cell"
    android:layout_width="@dimen/cell_two_view_holder_cell_height"
    android:layout_height="@dimen/cell_two_view_holder_cell_height">

    <ImageView
        android:id="@+id/photo_temp_image_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/cell_two_view_holder_photo_temp_image_view_margin_start"
        android:layout_marginRight="@dimen/cell_two_view_holder_photo_temp_image_view_margin_end"
        android:scaleType="center"
        android:src="@drawable/photo_temp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="-7dp"/>
</android.support.constraint.ConstraintLayout>